{
  "manifest_version": 3,
  "version": "1.0",
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "permissions": ["activeTab", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "/assets/Icon.png",
      "16": "/assets/Icon16.png",
      "128": "/assets/Icon128.jpeg"
    }
  },
  "default_locale": "en"
}
